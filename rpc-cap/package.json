  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/node": "^12.0.7",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "clone": "^2.1.2",
    "eslint": "^6.8.0",
    "fast-deep-equal": "^2.0.1",
    "nyc": "^15.0.0",
    "tape": "^4.9.2",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@onekeyhq/extension-controllers": "^5.1.0",
